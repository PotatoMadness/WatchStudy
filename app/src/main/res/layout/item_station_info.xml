<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="5dp"
    android:layout_marginBottom="15dp"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:orientation="vertical"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_distance"
            android:text="0.0km"
            android:textSize="12sp"
            android:lineHeight="6sp"
            android:textColor="#ABACAF"
            android:layout_marginTop="6dp"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_operator"
            android:text="충전사업자명"
            android:textColor="#797A7D"
            android:layout_marginTop="4dp"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            android:id="@+id/tv_snm"
            android:text="충전소이름 충전소이름 충전소이름"
            android:textSize="16sp"
            android:textAlignment="center"
            android:textColor="#ffffff"
            android:layout_marginTop="4dp"/>
        <androidx.appcompat.widget.LinearLayoutCompat
            android:id="@+id/ll_speed_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:gravity="center"
            android:orientation="horizontal">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/icon_electric"
                app:tint="#ABACAF" />
            <TextView
                android:id="@+id/tv_first_speed"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="50kW"
                android:textColor="#ABACAF"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tv_first_status"
                android:layout_marginStart="4dp"
                android:text="1/4"
                android:textColor="#ABACAF"/>
        </androidx.appcompat.widget.LinearLayoutCompat>
        <androidx.appcompat.widget.LinearLayoutCompat
            android:id="@+id/ll_speed_2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_marginTop="2dp"
            android:orientation="horizontal">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/icon_electric"
                app:tint="#ABACAF" />
            <TextView
                android:id="@+id/tv_second_speed"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="완속"
                android:textColor="#ABACAF"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tv_second_status"
                android:layout_marginStart="4dp"
                android:text="1/4"
                android:textColor="#ABACAF"/>
        </androidx.appcompat.widget.LinearLayoutCompat>
    </androidx.appcompat.widget.LinearLayoutCompat>

</androidx.constraintlayout.widget.ConstraintLayout>